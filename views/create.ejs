  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <link rel="stylesheet" type="text/css" href="stylesheets/style.css">

</head>
<body>

<!-- Navbar -->
<nav class="navbar navbar-inverse">
    <div class="container-fluid">
      <div class="navbar-header">
            <a class="navbar-brand" href="/index">
                    <img src="" width="80" height="80" alt="">
                  </a>
      </div>
      <ul class="nav navbar-nav">
        <li><a href="/">Home</a></li>
        <li><a href="/aboutus">About us</a></li>
        <li><a href="/contactus">Contact</a></li>
        <li class="active"><a href="/createbattle">Create Battle</a></li>
        <li><a href="/findbattle">Find Battle</a></li>
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <li><a href="/signup"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
        <li><a href="/login"><span class="glyphicon glyphicon-log-in"></span> Log In</a></li>
      </ul>
    </div>
  </nav>

  <form>
      <div class="container-fluid bg-12">
      <fieldset>
        <legend>Create</legend>
        </div>
        <div class="container-fluid bg-13">
        <div class="form-group col-md-6 text-left">
            <label for="inputEmail4">Evento</label>
            <input type="email" placeholder="nombre del evento">
          </div>

                  <div class="form-group">
                      <label for="inputEmail4">Email</label>
                      <input type="email" placeholder="Email">
                      <id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
                    </div>
              
        <div class="form-group">
          <label for="exampleTextarea">Descripción del evento</label>
          <textarea class="form-control" id="exampleTextarea" rows="3"></textarea>
        </div>

        <div class="form-group">
            <label for="exampleSelect1">Nivel de dificultad</label>
            <select class="form-control" id="exampleSelect1">
              <option>1</option>
              <option>2</option>
              <option>3</option>
              <option>4</option>
              <option>5</option>
            </select>
          </div>

          <div class="form-group">
              <label for="inputAddress">Address</label>
              <input type="text" class="form-control" id="inputAddress" placeholder="1234 Av.San Luis">
            </div>

            <div class="form-group">
                <label for="exampleTextarea">Descripción del lugar</label>
                <textarea class="form-control" id="exampleTextarea" rows="2"></textarea>
              </div>

        <div class="form-group">
          <label for="exampleInputFile">Seleccionar foto para el evento</label>
          <input type="file" class="form-control-file" id="exampleInputFile" aria-describedby="fileHelp">
          <small id="fileHelp" class="form-text text-muted"></small>
        </div>
       
      <div class="form-group">
        <div class="form-check">
          <input class="form-check-input" type="checkbox" id="gridCheck">
          <label class="form-check-label" for="gridCheck">
            Check me out
          </label>
        </div>
      </div>
      <button type="submit" class="btn btn-primary">Create</button>
    </form>
  
  </div>

  <!-- Footer -->
  <footer class="container-fluid bg-4 text-center">
    <p> <a href=""></a></p> 
  </footer>

</body>
